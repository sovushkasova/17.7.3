money = int (input( "Введите сумму: "))
per_cent = {'ТКБ': 5.6, 'СКБ': 5.9, 'ВТБ': 4.28, 'СБЕР': 4.0}
valuesList = []
for key in per_cent:
    valuesList.append(per_cent[key])
    one = (valuesList)[0:1]
    two = (valuesList)[1:2]
    three = (valuesList)[2:3]
    four = (valuesList)[3:]
odin = float(''.join(map(str, one)))
dwa = float(''.join(map(str, two)))
tri = float(''.join(map(str, three)))
chet = float(''.join(map(str, four)))

final = [int((money * odin/100)), int((money * dwa/100)), int((money * tri/100)), int((money * chet/100)),]
deposit = str(', '.join(map(str, final)))
print ("Накопленные средства за год вклада в каждом из банков:",(deposit))

deposit_i = max(final)

print("Максимальная сумма, которую вы можете заработать:",(deposit_i))
